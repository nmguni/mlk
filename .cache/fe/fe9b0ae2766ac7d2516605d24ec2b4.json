.quoteBorder {\r\n  border: 5px solid #00000033;\r\n  padding: 0.5rem;\r\n  border-radius: 50%;\r\n}\r\n\r\n// Section one----------------\r\n.sectionOne {\r\n  height: 100vh;\r\n  display: grid;\r\n  grid-template-columns: 45% 55%;\r\n  letter-spacing: 0.1rem;\r\n  .sectionOne__content {\r\n    color: #000000d2;\r\n    nav {\r\n      padding: 3% 5% 0 5%;\r\n      text-transform: capitalize;\r\n      ul {\r\n        list-style-type: none;\r\n        margin: 0;\r\n        padding: 0;\r\n        a {\r\n          text-decoration: none;\r\n          color: #000;\r\n        }\r\n        li {\r\n          display: inline;\r\n          margin: 0 3%;\r\n          float: right;\r\n\r\n          &:hover {\r\n            border-bottom: 1px solid #000000d2;\r\n          }\r\n        }\r\n        .nav--logo {\r\n          float: left;\r\n          font-weight: bold;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .content__text {\r\n    padding: 0 5% 0 10%;\r\n    position: absolute;\r\n    top: 5rem;\r\n\r\n    h1 {\r\n      font-size: 9vw;\r\n      color: rgba(0, 0, 0, 0.83);\r\n    }\r\n    p {\r\n      margin: 0 60% 0 0;\r\n      line-height: 1.8rem;\r\n      color: rgba(53, 53, 53, 0.77);\r\n    }\r\n  }\r\n  .sectionOne__btmBox {\r\n    position: absolute;\r\n    bottom: 0;\r\n    background-color: rgba(53, 53, 53, 0.48);\r\n    height: 8rem;\r\n    width: 60%;\r\n\r\n    display: grid;\r\n    grid-template-columns: repeat(4, 1fr);\r\n    .Text {\r\n      display: flex;\r\n      flex-direction: column;\r\n      justify-content: center;\r\n      align-items: center;\r\n\r\n      font-weight: bold;\r\n      font-size: 1.3rem;\r\n    }\r\n  }\r\n}\r\n.sectionOne__img {\r\n  background-image: url(\"../images/March\\ on\\ Washington3.jpg\");\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  background-position: 50% 50%;\r\n\r\n  // transform: scaleY(0);\r\n  // transform-origin: bottom;\r\n  // opacity: 0;\r\n}\r\n\r\n// about ----------------------\r\n\r\n// section two ----------------\r\n.sectionTwo {\r\n  height: 100vh;\r\n  display: grid;\r\n  grid-template-columns: 45% 55%;\r\n\r\n  .sectionTwo--content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background-color: rgba(53, 53, 53, 0.48);\r\n\r\n    p {\r\n      line-height: 1.8rem;\r\n      color: #ffffffad;\r\n      padding: 20% 30% 0 5%;\r\n      letter-spacing: 0.1rem;\r\n    }\r\n  }\r\n\r\n  .sectionTwo--img {\r\n    background-image: url(\"../images/martin-luther-speech.jpg\");\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    z-index: -10;\r\n    // position: relative;\r\n\r\n    .text {\r\n      font-size: 9vw;\r\n      color: rgba(0, 0, 0, 0.83);\r\n      font-weight: bold;\r\n      position: absolute;\r\n      margin-top: 10%;\r\n      margin-left: 10%;\r\n    }\r\n  }\r\n}\r\n\r\n// -------------  SECTION THREE    ----------\r\n.sectionThree {\r\n  height: 100vh;\r\n  display: grid;\r\n  grid-template-columns: 45% 55%;\r\n\r\n  .sectionThree--img {\r\n    background-image: url(\"../images/martin-luther-flag.jpg\");\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    background-position: 30% 80%;\r\n  }\r\n  .sectionThree--content {\r\n    .text {\r\n      font-size: 9vw;\r\n      color: rgba(0, 0, 0, 0.83);\r\n      font-weight: bold;\r\n      position: absolute;\r\n      margin-top: 2%;\r\n      margin-left: 5%;\r\n    }\r\n    p {\r\n      line-height: 1.8rem;\r\n      // color: #ffffffad;\r\n      margin: 35% 2% 0 23%;\r\n      letter-spacing: 0.1rem;\r\n    }\r\n\r\n    .sectionOne__btmBox {\r\n      position: relative;\r\n      bottom: 0;\r\n      background-color: rgba(53, 53, 53, 0.48);\r\n      height: 8rem;\r\n      width: 140%;\r\n      bottom: 0;\r\n      display: grid;\r\n      grid-template-columns: repeat(4, 1fr);\r\n    }\r\n  }\r\n}\r\n\r\n// -------------  SECTION FOUR    ----------\r\n.sectionFour {\r\n  height: 10