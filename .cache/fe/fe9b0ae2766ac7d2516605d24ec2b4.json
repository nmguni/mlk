.nav-container {\r\n  // max-width: 1000px;\r\n  text-align: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  position: relative;\r\n  z-index: 10;\r\n}\r\n.main-content {\r\n  min-height: 100vh;\r\n}\r\nnav {\r\n  padding-top: 4%;\r\n\r\n  ul {\r\n    list-style: none;\r\n    text-transform: uppercase;\r\n\r\n    margin: 0;\r\n    padding: 0;\r\n\r\n    li {\r\n      float: left;\r\n      position: relative;\r\n\r\n      a {\r\n        display: block;\r\n        margin: 0 50px;\r\n        padding: 0 10px;\r\n        font-weight: 600;\r\n        letter-spacing: 2px;\r\n        color: #000;\r\n        text-decoration: none;\r\n        &:hover {\r\n          color: rgba(0, 0, 0, 0.15);\r\n          border-bottom: rgba(0, 0, 0, 0.15) solid 1px;\r\n          border-left: rgba(0, 0, 0, 0.15) solid 1px;\r\n        }\r\n\r\n        &:visited {\r\n          display: block;\r\n          padding: 0 40px;\r\n          color: #000;\r\n          text-decoration: none;\r\n        }\r\n\r\n        &:not(:only-child):after,\r\n        &:visited:not(:only-child):after {\r\n          padding-left: 4px;\r\n          content: \" â–¾\";\r\n        }\r\n      }\r\n\r\n      ul li {\r\n        min-width: 190px;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.nav-dropdown {\r\n  position: absolute;\r\n  display: none;\r\n  z-index: 1;\r\n  box-shadow: 0 3px 12px rgba(0, 0, 0, 0.15);\r\n}\r\n\r\n/* Mobile navigation */\r\n\r\n// .nav-mobile {\r\n//   display: none;\r\n//   position: absolute;\r\n//   top: 0;\r\n//   right: 0;\r\n//   background: #262626;\r\n//   height: 40px;\r\n//   width: 40px;\r\n//   border-radius: 4px;\r\n// }\r\n\r\n// @media only screen and (max-width: 600px) {\r\n//   .nav-mobile {\r\n//     display: block;\r\n//   }\r\n\r\n//   nav {\r\n//     width: 100%;\r\n//     padding: 70px 0 15px;\r\n\r\n//     ul {\r\n//       display: none;\r\n\r\n//       li {\r\n//         float: none;\r\n\r\n//         a {\r\n//           padding: 15px;\r\n//           line-height: 20px;\r\n//         }\r\n\r\n//         ul li a {\r\n//           padding-left: 30px;\r\n//         }\r\n//       }\r\n//     }\r\n//   }\r\n\r\n//   .nav-dropdown {\r\n//     position: static;\r\n//   }\r\n// }\r\n\r\n// @media screen and (min-width: 799px) {\r\n//   .nav-list {\r\n//     display: block !important;\r\n//   }\r\n// }\r\n\r\n#nav-toggle {\r\n  position: absolute;\r\n  left: 7px;\r\n  top: 7px;\r\n  cursor: pointer;\r\n  padding: 10px 35px 16px 0px;\r\n\r\n  span {\r\n    cursor: pointer;\r\n    border-radius: 1px;\r\n    height: 5px;\r\n    width: 25px;\r\n    background: #fff;\r\n    position: absolute;\r\n    display: block;\r\n    content: \"\";\r\n    transition: all 300ms ease-in-out;\r\n    border-radius: 4px;\r\n\r\n    &:before,\r\n    &:after {\r\n      cursor: pointer;\r\n      border-radius: 1px;\r\n      height: 5px;\r\n      width: 25px;\r\n      background: #fff;\r\n      position: absolute;\r\n      display: block;\r\n      content: \"\";\r\n      transition: all 300ms ease-in-out;\r\n      border-radius: 4px;\r\n    }\r\n\r\n    &:before {\r\n      top: -10px;\r\n    }\r\n\r\n    &:after {\r\n      bottom: -10px;\r\n    }\r\n  }\r\n\r\n  &.active span {\r\n    background-color: transparent;\r\n\r\n    &:before,\r\n    &:after {\r\n      top: 0;\r\n    }\r\n\r\n    &:before {\r\n      transform: rotate(45deg);\r\n    }\r\n\r\n    &:after {\r\n      transform: rotate(-45deg);\r\n    }\r\n  }\r\n}\r\n\r\n// article {\r\n//   max-width: 1000px;\r\n//   margin: 0 auto;\r\n//   padding: 10px;\r\n// }\r\n\r\n/* other */\r\n\r\n#ml-logo {\r\n  font-size: 2rem;\r\n  font-family: \"East Sea Dokdo\", cursive;\r\n}\r\n\r\n// main content\r\n.overplay {\r\n  position: absolute;\r\n  background-color: #ccc;\r\n  height: 100vh;\r\n  width: 100%;\r\n  z-index: 10;\r\n  opacity: 0.3;\r\n}\r\n.bg-color {\r\n  // background-color: #0c8448;\r\n  height: 500px;\r\n  margin-top: 8%;\r\n  position: absolute;\r\n  z-index: -10;\r\n  width: 100%;\r\n}\r\n.text {\r\n  // font-family: \"Baskervville\", serif;\r\n  font-size: 12vw;\r\n  text-transform: uppercase;\r\n  position: absolute;\r\n  z-index: 5;\r\n  bottom: 50%;\r\n  margin: 0 13%;\r\n  letter-spacing: 2rem;\r\n  color: #000;\r\n  // text-align: center;\r\n  h1,\r\n  p {\r\n    text-shadow: rgb(92, 92, 92) 1px 7px 10px;\r\n  }\r\n}\r\n.image {\r\n  text-align: center;\r\n  position: absolute;\r\n  bottom: 0;\r\n  margin: 0 20%;\r\n  // filter: brightness(50%);\r\n}\r\n\r\n// ------------------------------------------------------------------\r\n//---------------------------------------------------------------------\r\n#history {\r\n  min-height: 100%;\r\n  // margin-top: 45%;\r\n}\r\n.grid {\r\n  // margin-top: 35%;\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nfigure {\r\n  margin: 0;\r\n}\r\n\r\nfigcaption {\r\n  font-style: italic;\r\n  font-size: 0.8em;\r\n  /* color: grey; */\r\n  line-height: 1.4;\r\n  padding-top: 0;\r\n}\r\n\r\nh4 {\r\n  margin: 0;\r\n}\r\n.content {\r\n  width: 90%;\r\n}\r\n\r\n.grid {\r\n  display: grid;\r\n  margin: 0 6% 0 6%;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  grid-gap: 2em;\r\n  overflow-x: scroll;\r\n  -webkit-overflow-scrolling: touch;\r\n  padding: 2em 1em;\r\n}\r\n\r\n.grid__figure {\r\n  display: grid;\r\n  grid-template-rows: 15em 1fr;\r\n  grid-gap: 1em;\r\n}\r\n.grid__figure img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n/* ////////////////////\r\n */\r\n\r\n.date {\r\n  font-size: 7rem;\r\n  position: absolute;\r\n  font-weight: 900;\r\n  margin-top: 11%;\r\n  color: rgba(0, 0, 0, 0.562);\r\n  padding-left: 3%;\r\n}\r\n\r\n.date2 {\r\n  font-size: 7rem;\r\n  font-weight: 900;\r\n  position: absolute;\r\n  color: rgba(0, 0, 0, 0.562);\r\n  margin-top: 11%;\r\n  padding-left: 3%;\r\n}\r\n.date3 {\r\n  font-size: 7rem;\r\n  font-weight: 900;\r\n  position: absolute;\r\n  color: rgba(0, 0, 0, 0.562);\r\n  margin-top: 11%;\r\n  padding-left: 3%;\r\n}\r\nimg {\r\n  filter: grayscale(1);\r\n  border-radius: 3px;\r\n  box-shadow: rgba(0, 0, 0, 0.256) 3px 3px 3px;\r\n}\r\n#imgWidth {\r\n  width: 50%;\r\n  margin-left: 20%;\r\n}\r\nimg:hover {\r\n  filter: grayscale(0);\r\n}\r\n\r\n.date.grid__figure:hover {\r\n  color: rgb(0, 0, 0);\r\n}\r\n\r\n// laptop 1024px\r\n@media screen and (max-width: 1200px) {\r\n  .text {\r\n    font-size: 14vw;\r\n    bottom: 55%;\r\n    margin: 0 4%;\r\n  }\r\n  .image {\r\n    width: 850px;\r\n    margin: 0 9%;\r\n  }\r\n}\r\n\r\n// tablet 768px\r\n@media screen and (max-width: 780px) {\r\n  .navigation {\r\n    margin: 0;\r\n  }\r\n  .text {\r\n    font-size: 14vw;\r\n    bottom: 45%;\r\n    margin: 0 2%;\r\n  }\r\n  .image {\r\n    width: 700px;\r\n    margin: 0 5%;\r\n  }\r\n}\r\n\r\n// tablet 450px\r\n@media screen and (max-width: 450px) {\r\n  .content {\r\n    .text {\r\n      margin-left: 5%;\r\n      bottom: 35%;\r\n      font-size: 10vw;\r\n    }\r\n  }\r\n\r\n  .navigation {\r\n    padding-top: 10%;\r\n    ul {\r\n      li {\r\n        a {\r\n          margin: 0 5%;\r\n          font-size: 1rem;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .text {\r\n    // margin-right: 15%;\r\n    h1 {\r\n      font-size: 10vw;\r\n      margin-left: 12%;\r\n    }\r\n    // font-size: 1rem;\r\n    // bottom: 60%;\r\n    // margin-left: 15%;\r\n  }\r\n  #bgImgOne {\r\n    display: none;\r\n  }\r\n  #mobileText {\r\n    margin-left: 12%;\r\n    // font-size: 1vw;\r\n    font-weight: bold;\r\n    p {\r\n      padding: 20px 0 10px 0;\r\n    }\r\n  }\r\n  .date {\r\n    font-size: 1rem;\r\n  }\r\n  .date2 {\r\n    font-size: 1rem;\r\n  }\r\n  .date3 {\r\n    font-size: 1rem;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"39a84d9696bc59b866f7b1a0490e7641","cacheData":{"env":{}}}