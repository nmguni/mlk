ton.jpg\");\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  background-position: 50% 50%;\r\n}\r\n\r\n//-----------------------\r\n\r\n//        MEDIA Q'S\r\n\r\n//-----------------------\r\n\r\n@media only screen and (max-width: 1250px) {\r\n  .sectionOne {\r\n    .sectionOne--content {\r\n      color: #000000d2;\r\n      nav {\r\n        padding: 3% 5% 0 5%;\r\n        text-transform: capitalize;\r\n\r\n        ul {\r\n          list-style-type: none;\r\n          writing-mode: vertical-rl;\r\n          position: absolute;\r\n          display: inherit;\r\n          margin: 5% 0 0 1.8rem;\r\n          padding: 0;\r\n          a {\r\n            text-decoration: none;\r\n            color: #000;\r\n          }\r\n          li {\r\n            margin: 1.5rem 0;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 1000px) {\r\n  .sectionOne {\r\n    .sectionOne--content {\r\n      color: #000000d2;\r\n      nav {\r\n        ul {\r\n          display: none;\r\n        }\r\n      }\r\n    }\r\n\r\n    .sectionOne--btmBox {\r\n      .Text {\r\n        font-size: 1rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 900px) {\r\n  .sectionOne--btmBox {\r\n    width: 100%;\r\n    .Text {\r\n      font-size: 1rem;\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 800px) {\r\n  .sectionOne {\r\n    .content--text {\r\n      padding: 0 5% 0 5%;\r\n      p {\r\n        margin: 0 55% 0 0;\r\n        line-height: 1.8rem;\r\n        padding-top: 0.5rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 700px) {\r\n  .sectionOne {\r\n    .sectionOne--btmBox {\r\n      width: 100%;\r\n      .Text {\r\n        // position: absolute;\r\n        bottom: 40%;\r\n        // padding-right: 5%;\r\n        // margin-left: 62%;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 550px) {\r\n  .sectionOne {\r\n    .sectionOne--btmBox {\r\n